msgid "Astrid will vibrate when sending notifications"
msgstr "传送通知时会震动"

#. Reminder Preference: Vibrate Description (false)
msgctxt "rmd_EPr_vibrate_desc_false"
msgid "Astrid will not vibrate when sending notifications"
msgstr "传送通知震动关闭"

